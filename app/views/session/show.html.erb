<aside>

    <h2>User Data</h2>

    <table>
           <tr>
                   <td>Auth Provider: </td>
                   <td><%= @auth["provider"] %></td>
           </tr>
           <tr>
                   <td>Unique ID: </td>
                   <td><%= @auth["uid"] %></td>
           </tr>
           <tr>
                   <td>Email: </td>
                   <td><%= @auth["info"]["email"] %></td>
           </tr>
           <tr>
                   <td>Username: </td>
                   <td><%= @auth["info"]["nickname"] %></td>
           </tr>
           <tr>
                   <td>Name: </td>
                   <td><%= @auth["info"]["name"] %></td>
           </tr>
           <tr>
                   <td>Mugshot: </td>
                   <td><img src="<%= @auth["info"]["image"] %>" width="50" /></td>
           </tr>
    </table>
</aside>

<main>

    <h2>Facebook Feed</h2>

    <%= render 'feed' %>

</main>
